{
"compilerOptions": {
  "module": "commonjs",
  "target": "es2015",
  "rootDir": ".",
  "outDir": "./dist",
  "noImplicitAny": false,
  "sourceMap": false,
  "emitDecoratorMetadata": true,
  "experimentalDecorators": true,
  "moduleResolution": "node",
  "declaration": false,
  "listFiles": false
},
"exclude": [
  ".vscode",
  ".serverless",
  ".git",
  "node_modules"
],
"compileOnSave": false,
"buildOnSave": false,
"atom": {
  "rewriteTsconfig": false
}
}